def bas_diaqonal_kvadratlar_cemi(A):
    n = len(A)  # Matrisin ölçüsü (n x n)
    cem = 0
    for i in range(n):
        cem += A[i][i] ** 2  # Baş diaqonal element: A[i][i]
    return cem

# Nümunə istifadə:
A = [
    [2, 3, 4],
    [5, 6, 7],
    [8, 9, 1]
]

print("Baş diaqonal elementlərin kvadratları cəmi:", bas_diaqonal_kvadratlar_cemi(A))
